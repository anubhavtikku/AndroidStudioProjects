<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mahe.fifth.com.intentdatatransfer">

    <application
        android:allowBackup="true"
        android:icon="@drawable/scenery"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProfileActivity" />
        <activity android:name=".TouristPlaces" />
        <activity android:name=".TajMahal" />
        <activity
            android:name=".Himalayas"
            android:label="@string/title_activity_himalayas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RedFort"
            android:label="@string/title_activity_red_fort"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".IndiaGate"></activity>
    </application>

</manifest>